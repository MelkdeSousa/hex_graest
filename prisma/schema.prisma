// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Book {
  id     String   @id
  title  String
  author Author[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Author {
  id    String @id
  name  String
  books Book[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
